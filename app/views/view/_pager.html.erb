<div class="pager">
  <% @pages.each do |key, prefs| %>
    <% start_date = Date.parse(prefs[:start_date].to_s) rescue prefs[:start_date] %>
    <% end_date = Date.parse(prefs[:end_date].to_s) rescue prefs[:end_date] %>
    <%= link_to("#{start_date} - #{end_date}", get_permalink(page: key, per_page: page_limit),
      class: (@current_page == prefs) ? 'active' : 'passive') %>
  <% end %>
</div>
